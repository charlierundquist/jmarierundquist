{
  "name": "jmarierundquist-redesign",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "copy:static": "cp -r .next/static .next/standalone/.next/",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@deemlol/next-icons": "^0.1.9",
    "@eslint/eslintrc": "^3.3.1",
    "@libsql/client": "^0.15.12",
    "@libsql/linux-x64-musl": "^0.5.17",
    "@payloadcms/db-postgres": "3.46.0",
    "@payloadcms/db-sqlite": "3.46.0",
    "@payloadcms/email-resend": "3.46.0",
    "@payloadcms/live-preview-react": "3.46.0",
    "@payloadcms/next": "3.46.0",
    "@payloadcms/payload-cloud": "3.46.0",
    "@payloadcms/plugin-cloud-storage": "3.46.0",
    "@payloadcms/plugin-form-builder": "3.46.0",
    "@payloadcms/richtext-lexical": "3.46.0",
    "@payloadcms/storage-s3": "3.46.0",
    "@payloadcms/storage-uploadthing": "3.46.0",
    "@payloadcms/ui": "3.46.0",
    "@tailwindcss/postcss": "^4.1.11",
    "blurhash": "^2.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "libsql": "^0.5.17",
    "motion": "^12.23.12",
    "next": "15.3.2",
    "payload": "3.46.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^4.8.0",
    "sharp": "0.34.2",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "4.9.5"
  },
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.12.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
